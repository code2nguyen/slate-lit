{
  "name": "@c2n/slate-lit",
  "version": "1.0.0",
  "description": "Webcomponent slate-lit following open-wc recommendations",
  "author": "slate-lit",
  "license": "MIT",
  "main": "index.js",
  "module": "index.js",
  "scripts": {
    "build": "tsc",
    "rollup": "rollup -c",
    "start": "concurrently --kill-others --names tsc,web-dev-server \"npm run tsc:watch\" \"web-dev-server --app-index demo/index.html --node-resolve --open --watch\"",
    "tsc:watch": "tsc --watch",
    "prepublish": "rm -rf dist && tsc && npm run rollup && cp package-publish.json  dist/package.json",
    "storybook": "start-storybook --node-resolve --watch --open",
    "storybook:build": "build-storybook -o docs"
  },
  "dependencies": {
    "direction": "^1.0.4",
    "is-hotkey": "^0.1.6",
    "lit-element": "^2.2.1",
    "lit-html": "^1.1.2",
    "lodash-es": "^4.17.15",
    "slate": "^0.59.0",
    "slate-history": "^0.59.0"
  },
  "devDependencies": {
    "@open-wc/demoing-storybook": "^2.4.6",
    "@types/direction": "^1.0.0",
    "@types/is-hotkey": "^0.1.2",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "13.11.1",
    "@rollup/plugin-replace": "^2.3.1",
    "@rollup/plugin-typescript": "^6.0.0",
    "@web/dev-server": "^0.0.12",
    "@web/dev-server-esbuild": "^0.2.6",
    "concurrently": "^5.1.0",
    "rimraf": "^3.0.2",
    "rollup": "^1.32.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-filesize": "^7.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-postcss": "^2.5.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript": "^1.0.1",
    "tslib": "2.0.3",
    "typescript": "~4.0.3",
    "web-component-analyzer": "^1.1.6"
  }
}
