{
  "name": "slate-lit",
  "version": "0.0.0",
  "description": "Webcomponent slate-lit following open-wc recommendations",
  "author": "slate-lit",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "scripts": {
    "start": "concurrently --kill-others --names tsc,web-dev-server \"npm run tsc:watch\" \"web-dev-server --app-index demo/index.html --node-resolve --open --watch\"",
    "tsc:watch": "tsc --watch",
    "prepublish": "tsc",
    "storybook": "start-storybook --node-resolve --watch --open",
    "storybook:build": "build-storybook -o docs"
  },
  "dependencies": {
    "@open-wc/demoing-storybook": "^2.4.6",
    "@types/lodash-es": "^4.17.3",
    "direction": "^1.0.4",
    "is-hotkey": "^0.1.6",
    "lit-element": "^2.2.1",
    "lit-html": "^1.1.2",
    "lodash-es": "^4.17.15",
    "slate": "^0.59.0",
    "slate-history": "^0.59.0"
  },
  "devDependencies": {
    "@types/direction": "^1.0.0",
    "@types/is-hotkey": "^0.1.2",
    "@types/node": "13.11.1",
    "@web/dev-server": "^0.0.12",
    "@web/dev-server-esbuild": "^0.2.6",
    "concurrently": "^5.1.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "tslib": "^1.11.0",
    "typescript": "~4.0.3",
    "web-component-analyzer": "^1.1.6"
  }
}
